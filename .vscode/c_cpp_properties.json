{
    "configurations": [
        {
            "name": "RISC-V (Clang)",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/clang-16",
            "cStandard": "c11",
            "intelliSenseMode": "clang-riscv32",
			"compilerArgs": [
				"-std=c11 -O2 -g3 -Wall -Wextra --target=riscv32 -ffreestanding -nostdlib -mno-relax"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}